<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>CaddieServlet.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>caddieServlet</servlet-name>
        <servlet-class>CaddieServlet.caddieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>payServlet</servlet-name>
        <servlet-class>CaddieServlet.payServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>caddieServlet</servlet-name>
        <url-pattern>/caddie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>payServlet</servlet-name>
        <url-pattern>/pay</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
