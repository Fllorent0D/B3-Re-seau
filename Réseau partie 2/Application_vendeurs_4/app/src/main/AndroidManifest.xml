<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bastin.application_vendeurs_4">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DisponibilityActivity" />
        <activity android:name=".activity.LogBookActivity" />
        <activity android:name=".activity.ReservationActivity" />
        <activity android:name=".activity.SaleInfoActivity" />
        <activity android:name=".activity.StatsActivity"></activity>
    </application>

</manifest>